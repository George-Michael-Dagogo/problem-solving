    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2.82 µs ± 552 ns per loop (mean ± std. dev. of 7 runs, 100000 loops each)\n"
     ]
    }
   ],
   "source": [
    "%%timeit\n",
    "#Normal Python loop\n",
    "a = [2,3,4,5,6,3,5,6,4,2,6,7,8,9,22,33,44,55]\n",
    "b = []\n",
    "for i in a:\n",
    "    if i % 2 != 1:\n",
    "        b.append(i)\n",
    "    "
   ]
