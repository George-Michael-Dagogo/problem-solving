   "source": [
    "# *args **kwargs speed test"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "({'Google': 8, 'Microsoft': 7, 'Apple': 8.5, 'Dangote': 9.5}, dict)"
      ]
     },
     "execution_count": 30,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def calculate(*numb):\n",
    "    # *args for variable number of arguments\n",
    "    total = 0\n",
    "    for num in numb:\n",
    "        total += num\n",
    "    return total \n",
    "def word(**eng):\n",
    "   # **kwargs with ** for variable number of keyword arguments\n",
    "    return(eng, type(eng))\n",
    {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Python coding vs pythonic coding "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import timeit"
   ]
  },
